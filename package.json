{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "scripts": {
    "develop": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js -w",
    "init": "run-s create-ppk move-ppk",
    "start": "run-p watch upload",
    "upload": "kintone-plugin-uploader dist/plugin-dev.zip --watch --waiting-dialog-ms 3000",
    "update-modules": "ncu -u",
    "check-updates": "ncu",
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.js"
  },
  "devDependencies": {
    "@emotion/css": "11.1.3",
    "@emotion/styled": "11.3.0",
    "@kintone/dts-gen": "5.0.8",
    "@kintone/plugin-packer": "5.0.22",
    "@kintone/plugin-uploader": "6.0.5",
    "@kintone/rest-api-client": "2.0.23",
    "@kintone/webpack-plugin-kintone-plugin": "5.0.20",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@types/luxon": "2.0.5",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "immer": "9.0.6",
    "luxon": "2.0.2",
    "notistack": "2.0.2",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "0.4.1",
    "terser-webpack-plugin": "5.2.4",
    "ts-loader": "9.2.6",
    "ts-node": "10.2.1",
    "typescript": "4.4.3",
    "webpack": "5.58.1",
    "webpack-cli": "4.9.0",
    "webpack-merge": "5.8.0"
  }
}
