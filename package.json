{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "scripts": {
    "develop": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js -w",
    "init": "run-s create-ppk move-ppk",
    "start": "run-p watch upload",
    "upload": "kintone-plugin-uploader dist/plugin-dev.zip --watch --waiting-dialog-ms 3000",
    "update-modules": "ncu -u",
    "check-updates": "ncu",
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts"
  },
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/styled": "^11.3.0",
    "@kintone/dts-gen": "^4.2.5",
    "@kintone/plugin-packer": "^5.0.7",
    "@kintone/plugin-uploader": "^5.0.4",
    "@kintone/rest-api-client": "^2.0.8",
    "@kintone/webpack-plugin-kintone-plugin": "^5.0.7",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@types/luxon": "^1.27.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "immer": "^9.0.3",
    "luxon": "^1.27.0",
    "notistack": "^1.0.9",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.3.1",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
